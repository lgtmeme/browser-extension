{
  "name": "LGTMeme",
  "version": "0.0.2",
  "description": "ItS SheRAmiE !",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent"],
  "optional_permissions": ["http://*/", "https://*/", "tabs"],
  "background": {
    "scripts": ["backgroundScript.bundle.js"],
    "persistent": false
  },
  "web_accessible_resources": [
    "pageContextScript.bundle.js"
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/browser_action_icon.png",
      "32": "assets/browser_action_icon-2x.png"
    },
    "default_popup": "popup.html",
    "default_title": "Shoop Da Woop!"
  }
}
